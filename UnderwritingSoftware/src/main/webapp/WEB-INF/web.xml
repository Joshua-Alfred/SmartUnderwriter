<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
	<display-name>Archetype Created Web Application</display-name>


	<servlet>
		<servlet-name>Upload</servlet-name>
		<servlet-class>com.uwriter.UploadServlet</servlet-class>
	</servlet>


	<servlet-mapping>
		<servlet-name>Upload</servlet-name>
		<url-pattern>/Uploadpage</url-pattern>

	</servlet-mapping>
	
	<servlet>
		<servlet-name>Uploadjsp</servlet-name>
		<servlet-class>/Process.jsp</servlet-class>
	</servlet>


	<servlet-mapping>
		<servlet-name>Uploadjsp</servlet-name>
		<url-pattern>/Process.jsp</url-pattern>

	</servlet-mapping>
</web-app>